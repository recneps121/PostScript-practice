% Reverses the order of the top 'k' number of items.
/reverseTopK {
	% Store the number of items to reverse and a counter.
	1 dict begin
	/k exch def
	/itr k def
	
	% Don't allow to large of a k value.
	k count 1 add lt {
		k 1 sub { 
			% Move the top item down itr number of spaces.
			itr 1 roll
			itr 0 gt {
				% Itr get's closer and closer to the top of the stack.
				/itr itr 1 sub def
			} if
		} repeat
	} if
	end
} def
